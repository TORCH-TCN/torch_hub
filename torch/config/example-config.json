{
    "collection": {
        "name": "BRIT",
        "prefix": "BRIT",
        "catalog_number_regex": "(?P<catNum>(?P<instID>BRIT)(-(?P<collID>XX)-)*(?P<numerical>\\d+))"
    },
    "upload": {
        "type": "sftp",
        "host": "torchhub.blob.core.windows.net",
        "path": "brit",
        "username": "torchhub.torchadmin",
        "password": "[contact Joseph for password]"
    },
    "generate_derivatives": {
        "regex": "(_(?P<suffix>a-z|0-9))*((_)(?P<size>thumb|med))?(\\.)(?i)(?P<ext>jpg|jpeg)",
        "sizes": {
            "full": {
                "output_path": "web/BRIT/"
            },
            "med": {
                "width": 900,
                "output_path": "web/BRIT/"
            },
            "thumb": {
                "width": 390,
                "output_path": "web/BRIT/"
            }
        },
        "JPG_RENAME_STRING": "unprocessed"
    },
    "SECRET_KEY": "TORCH1WEBAPP",
    "SQLALCHEMY_DATABASE_URI": "sqlite:///torch-hub.db",
    "SECURITY_REGISTERABLE": true,
    "SECURITY_PASSWORD_SALT": "torchsalt",
    "SECURITY_SEND_REGISTER_EMAIL": false,
    "SECURITY_RECOVERABLE": true,
    "SECURITY_CHANGEABLE": true,
    "SQLALCHEMY_TRACK_MODIFICATIONS": false,
    "MAIL_SERVER": "smtp.gmail.com",
    "MAIL_USERNAME": "antonio@kuviocreative.com",
    "MAIL_PASSWORD": "",
    "MAIL_DEFAULT_SENDER": "antonio@kuviocreative.com",
